<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <% if(locals.amp && amp) { %>
        <script async src="https://cdn.ampproject.org/v0.js"></script>
        <style amp-boilerplate>body{-webkit-animation:-amp-start 8s steps(1,end) 0s 1 normal both;-moz-animation:-amp-start 8s steps(1,end) 0s 1 normal both;-ms-animation:-amp-start 8s steps(1,end) 0s 1 normal both;animation:-amp-start 8s steps(1,end) 0s 1 normal both}@-webkit-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-moz-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-ms-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-o-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}</style><noscript><style amp-boilerplate>body{-webkit-animation:none;-moz-animation:none;-ms-animation:none;animation:none}</style></noscript>
    <% } %>
    <!-- Favicon -->
    <link rel="icon" type="image/png" href="/favicon/favicon-32x32.png" sizes="32x32" />
    <link rel="icon" type="image/png" href="/favicon/favicon-16x16.png" sizes="16x16" />
    <!-- Favicon -->

    <% if(!locals.amp || !amp) { %>
        <script type="text/javascript" src="/js/mobilehead.js"></script>
    <% } %>
    
    <!-- Styles -->
    <% if(!locals.amp || !amp) { %>
        <link rel='stylesheet' href='/stylesheets/web/main.css' />
        <link rel='stylesheet' href='/stylesheets/web/header.css' />
	    <link rel='stylesheet' href='/stylesheets/elements.css' />
    <% } %>
    
    <meta name="author" content="Erik Weisser">
	<meta name="publisher" content="Erik Weisser">
	<meta name="copyright" content="Erik Weisser">
	<meta name="page-topic" content="Development">
    <meta name="robots" content="index, follow">
    <meta charset="utf-8">


    
    <% if(locals.data && data.title) { %>
        <meta property="og:title" content="<%- data.title%>">
        <meta property="og:description" content="<%= data.description%>">
        <meta property="og:url" content="https://<%= altTag.replace('www.','').toUpperCase() %>/<%= data.id%>">
        <meta name="description" content="<%= data.description%>">
        <meta property="og:site_name" content="https://<%= altTag.replace('www.','').toLowerCase() %>/<%= data.id%>">
    <% } else { %> 
        <meta property="og:title" content="<%= altTag.replace('www.','').toUpperCase() %> - Dev Blog">
        <meta property="og:description" content="Are you interested in 'how to build a MicroService' or would you know how to deploy containers to the cloud with Jenkins? Find out all this and more on my dev blog!">
        <meta property="og:url" content="https://<%= altTag.replace('www.','').toUpperCase() %>">
        <meta name="description" content="Are you interested in 'how to build a MicroService' or would you know how to deploy containers to the cloud with Jenkins? Find out all this and more on my dev blog!">
        <meta property="og:site_name" content="https://<%= altTag.replace('www.','').toLowerCase() %>">
    <% } %>

    <link href="/rss.xml" title="RSS" type="application/rss+xml" rel="alternate">
    <link rel="canonical" href="<%= baseUrl %>">

    <meta property="og:image" content="https://<%= altTag.replace('www.','').toLowerCase() %>/images/meta/<%= altTag.replace('www.','').toLowerCase() %>.JPG">
</head>
