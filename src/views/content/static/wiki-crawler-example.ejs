<!DOCTYPE html>
<!-- saved from url=(0044)http://<%= altTag.toUpperCase() %>/elasticsearch-query-tester -->
<html><head><meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Favicon -->
    <link rel="icon" type="image/png" href="http://<%= altTag.toUpperCase() %>/favicon/favicon-32x32.png" sizes="32x32">
    <link rel="icon" type="image/png" href="http://<%= altTag.toUpperCase() %>/favicon/favicon-16x16.png" sizes="16x16">
    <!-- Favicon -->

    <!-- Styles -->
    <link rel="stylesheet" href="/example_search/default/header.css">
    <link rel="stylesheet" href="/example_search/default/header(1).css">
    <link rel="stylesheet" href="/example_search/default/elements.css">

    <!--/Styles -->
    <script src="/example_search/default/jquery-3.3.1.min.js"></script>

    <link rel="canonical" href="http://www.<%= altTag.toUpperCase() %>/elasticsearch-query-tester">

    <title>ELT - Elastic Query Tester</title>
    <link rel="stylesheet" type="text/css" href="/example_search/css/main.css">
    <script src="https://code.jquery.com/jquery-latest.js"></script>
    <script type="text/javascript" src="https://ajax.aspnetcdn.com/ajax/jquery.templates/beta1/jquery.tmpl.min.js"></script>
    <script type="text/javascript" src="/example_search/js/search.js"></script>

    <link rel="stylesheet" href="/example_search/lib/highlight/styles/railscasts.css">
    <script src="/example_search/lib/highlight/highlight.pack.js"></script>
    <script>hljs.initHighlightingOnLoad();</script>
    <!-- Place this tag in your head or just before your close body tag. -->
    <script async defer src="https://buttons.github.io/buttons.js"></script>
   


</head>
    <body style="">
        <div class="site-wrapper">
            <!-- Content -->
            <div class="site-main" style="margin-top: 30px">
    <script id="searchErrorTemplate" type="text/x-jquery-tmpl">
      <div class="search_result error">
       <div class="result_attribute">
         <b>Error:</b>
         <pre>{{html responseJSON}}</pre>      
        </div>
      </div>
    </script>
    <script id="searchInfoTemplate" type="text/x-jquery-tmpl">
      <div class="search_result">
        <div class="info_attribute">
          <b>SEARCH INFO:</b>
          <table>
            <tr>
              <td>
                visible entries:
              </td>
              <td>
                ${data.hits.hits.length}
              </td>
            </tr>
            <tr>
              <td>
                total entries:
              </td>
              <td>
                ${data.hits.total}
              </td>
            </tr>
            <tr>
              <td>
                search duration (ms):
              </td>
              <td>
                ${data.took}
              </td>
            </tr>
          </table>
        </div> 
        <div class="info_attribute">
          <b>QUERY:</b>
            <pre>{{html query}}</pre>
        </div>  
      </div>
    </script>
    <script id="searchResultTemplate" type="text/x-jquery-tmpl">
      {{each(i,result) hits}}
        <div class="search_result">
            {{each(i,key) result.dynamicResponseInfo.keys}}
              <div class="result_attribute">
                  {{if key != null}}
                    <b>${key}:</b>
                    ${result.dynamicResponseInfo.values[i]}
                  {{/if}}
             </div>
            {{/each}}
            <div class="result_attribute">
              <b>_source:</b>
              <table>
                {{each(i,key) result.dynamicResponse.keys}}
                  <tr>
                    <td class="generatedTableKey">
                      ${key}
                    </td>
                    <td class="generatedTableValue">
                      {{html result.dynamicResponse.values[i]}}
                    </td>
                  </tr>
                {{/each}}
              </table>
            </div>
        </div>
      {{/each}}
    </script>
    <h1>3L45T1C QU3RY T35T3R</h1>
    <section class="search">
      <div class="search_query">
        <textarea id="query" spellcheck="false">

{ 
   "q": "text:data.terms", 
   "size":"15" 
}

      </textarea>
      </div>


      <div class="search_box">
        <input id="search_txt" placeholder="WH47 4R3 Y0U L00k1N6 F0R?" autofocus />
        <div style="margin: 5px;">
        <a class="github-button" href="https://github.com/whit-e/wiki-crawler" data-size="large" aria-label="whit-e/wiki-crawler on GitHub">View on GitHub</a>
      </div>
      </div>

      

      <div class="search_hr"></div>
      
      <div id="error" class="search_info"></div>
      <div id="info" class="search_info"></div>
      <div id="results" class="search_results"></div>
    </section>  
  </div>
            <!-- /Content -->

        </div>

    
</body></html>